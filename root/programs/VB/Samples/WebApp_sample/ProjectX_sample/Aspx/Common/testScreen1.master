                    <br />
                    引き渡す情報を入力<br />
                    <cc1:WebCustomTextBox ID="TextBox3" runat="server"></cc1:WebCustomTextBox><br />
                    <cc1:WebCustomButton ID="btnMButton4" runat="server" Text="OK" Width="220px" ToolTip="OK" /><br />
