<?xml version="1.0" encoding="utf-8" ?>
<appSettings>
  <!-- ASP.NET Webアプリケーション用 -->

  <!-- パラメータ変更後は、iisresetコマンドを実行してください。 -->

  <!-- フレームワークの使用するパラメータ - start -->
  <!-- Controlのプレフィックス -->
  <add key="FxPrefixOfButton" value="btn"/>
  <add key="FxPrefixOfLinkButton" value="lbn"/>
  <add key="FxPrefixOfImageButton" value="ibn"/>
  <add key="FxPrefixOfImageMap" value="imp"/>
  <add key="FxPrefixOfDropDownList" value="ddl"/>
  <add key="FxPrefixOfListBox" value="lbx"/>
  <add key="FxPrefixOfRadioButton" value="rbn"/>
  <add key="FxPrefixOfRepeater" value="rpt"/>
  <add key="FxPrefixOfGridView" value="gvw"/>
  <add key="FxPrefixOfListView" value="lvw"/>
  <add key="FxPrefixOfRadioButtonList" value="rbl"/>
  <add key="FxPrefixOfCheckBoxList" value="cbl"/>
  <add key="FxPrefixOfTextBox" value="txt"/>
  <!-- 基盤画面パス -->
  <add key="FxErrorScreenPath" value="~/Aspx/Common/ErrorScreen.aspx"/>
  <add key="FxOKMessageDialogPath" value="~/Aspx/FrameWork/myOKMessageDialog.aspx"/>
  <add key="FxYesNoMessageDialogPath" value="~/Aspx/FrameWork/myYesNoMessageDialog.aspx"/>
  <add key="FxDialogFramePath" value="~/Aspx/Framework/DialogFrame.htm"/>
  <!-- アイコンパス -->
  <add key="FxInformationIconPath" value="~/images/touryo/information.ico"/>
  <add key="FxWarningIconPath" value="~/images/touryo/warning.ico"/>
  <add key="FxErrorIconPath" value="~/images/touryo/error.ico"/>
  <add key="FxQuestionIconPath" value="~/images/touryo/question.ico"/>
  <!-- デフォルトの画面スタイル -->
  <add key="FxDefaultFxDialogStyle" value="dialogWidth:450px;dialogHeight:250px;status:no;scroll:no;" />
  <add key="FxDefaultBusinessDialogStyle" value="dialogWidth:640px;dialogHeight:480px;status:no;" />
  <add key="FxDefaultNormalScreenStyle" value="width=960,height=700,scrollbars=yes" />
  <!-- セッションタイムアウト検出機能のon・off -->
  <add key="FxSessionTimeOutCheck" value="on"/>
  <!-- 二重送信防止機能のon・off -->
  <add key="FxDoubleTransmissionCheck" value="on"/>
  <!-- 不正操作防止機能のon・off（操作履歴の最大数） -->
  <add key="FxRequestTicketGuidMaxQueueLength" value="100"/>
  <!-- Button履歴を保持するキューの最大長 -->
  <add key="FxButtonhistoryMaxQueueLength" value="100"/>
  <!-- 親画面別セッション領域の自動削除機能のon・off（スコープの最大数） -->
  <add key="FxScreeenGuidMaxQueueLength" value="30"/>
  <!-- ブラウザ ウィンドウ別セッション領域の自動削除機能のon・off（スコープの最大数） -->
  <add key="FxWindowGuidMaxQueueLength" value="10"/>
  <!-- 画面遷移方法を指定（T：Transfer、R：Redirect、off） -->
  <add key="FxScreenTransitionMode" value="off"/>
  <!-- 画面遷移チェック機能のon・off -->
  <add key="FxScreenTransitionCheck" value="on"/>
  <!-- 共有情報定義へのパス -->
  <add key="FxXMLSPDefinition" value="C:\root\files\resource\XML\SPDefinition.xml"/>
  <!-- Message定義へのパス -->
  <add key="FxXMLMSGDefinition" value="C:\root\files\resource\XML\MSGDefinition.xml"/>
  <!-- 画面遷移定義へのパス -->
  <add key="FxXMLSCDefinition" value="C:\root\files\resource\XML\SCDefinition.xml"/>
  <!-- トランザクション制御定義へのパス -->
  <add key="FxXMLTCDefinition" value="C:\root\files\resource\XML\TCDefinition.xml"/>
  <!-- 名前解決定義へのパス -->
  <!-- In case of using IIS -->
  <!--<add key="FxXMLTMProtocolDefinition" value="C:\root\files\resource\XML\TMProtocolDefinition.xml"/>-->
  <!-- In case of using IIS Express -->
  <add key="FxXMLTMProtocolDefinition" value="C:\root\files\resource\XML\TMProtocolDefinition2.xml"/>
  <add key="FxXMLTMInProcessDefinition" value="C:\root\files\resource\XML\TMInProcessDefinition.xml"/>
  <!-- フレームワークの使用するパラメータ - end -->

  <!-- 共通部品の使用するパラメータ - start -->
  <!-- Log4Netのコンフィグファイルへのパス -->
  <add key="FxLog4NetConfFile" value="C:\root\files\resource\Log\SampleLogConf.xml"/>
  <!-- Ｄ層のSQLトレースログ出力機能のon・off -->
  <add key="FxSqlTraceLog" value="on"/>
  <!-- Ｄ層のSQL文キャッシュ機能のon・off -->
  <!-- 開発フェーズのことを考慮して、デフォルトoffに設定 -->
  <add key="FxSqlCacheSwitch" value="off"/>
  <!-- Ｄ層のSQLロード時のエンコーディングを指定（shift_jis、utf-8.etc） -->
  <add key="FxSqlEncoding" value="utf-8"/>
  <!-- Ｄ層のコマンド タイムアウト値を指定（秒） -->
  <add key="FxSqlCommandTimeout" value="30"/>
  <!-- 共通部品の使用するパラメータ - end -->
  
  <!--墨壷2で使用するパラメタ - start -->
  <!-- Daoクラス ファイルのヘッダ・フッタ -->
  <!-- Daoクラス名のヘッダ・フッタ -->
  <add key="DaoClassNameHeader" value="Dao"/>
  <add key="DaoClassNameFooter" value=""/>

  <!-- メソッド名のヘッダ・フッタ（静的） -->
  <add key="MethodNameHeaderS" value="S"/>
  <add key="MethodNameFooterS" value=""/>

  <!-- メソッド名のCRUD名 -->
  <add key="MethodLabel_Ins" value="1_Insert"/>
  <add key="MethodLabel_Sel" value="2_Select"/>
  <add key="MethodLabel_Upd" value="3_Update"/>
  <add key="MethodLabel_Del" value="4_Delete"/>
  <add key="MethodLabel_SelCnt" value="5_SelCnt"/>
  
  <!-- メソッド名のヘッダ・フッタ（動的） -->
  <add key="MethodNameHeaderD" value="D"/>
  <add key="MethodNameFooterD" value=""/>

  <!-- Updateのパラメタ名のヘッダ・フッタ -->
  <add key="UpdateParamHeader" value="Set_"/>
  <add key="UpdateParamFooter" value="_forUPD"/>

  <!-- Likeのパラメタ名のヘッダ・フッタ -->
  <add key="LikeParamHeader" value=""/>
  <add key="LikeParamFooter" value="_Like"/>
  <!--墨壷2で使用するパラメタ - end -->

  <!-- アプリケーションの使用するパラメータ - start -->
  <!-- OAuth2, OIDC認証 -->
  <add key="OAuth2AndOidcIsser" value="http://jwtssoauth.opentouryo.com"/>
  <add key="OAuth2AndOidcClientID" value="b6b393fe861b430eb4ee061006826b03"/>
  <add key="OAuth2AndOidcSecret" value="p2RgAFKF-JaF0A9F1tyDXp4wMq-uQZYyvTBM8wr_v8g"/>
  <add key="OAuth2AndOidcRS256Cer" value="C:\root\files\resource\X509\RS256.cer"/>

  <!-- ブラウザのタイトル -->
  <add key="BrowserTitle" value="フレームワーク テスト画面"/>
  <!-- 画面遷移方法（１がTransfer、２がRedirect） -->
  <add key="ScreenTransitionMethod" value="2"/>
  <!--Controlのプレフィックス（追加分） -->
  <add key="FxPrefixOfCheckBox" value="cbx"/>
  <!-- ファイル（フォルダ）へのパス -->
  <add key="SqlTextFilePath" value="C:\root\files\resource\Sql"/>
  <!-- キャッシュ制御機能のon・off -->
  <add key="FxCacheControl" value="ON"/>
  <!-- その他 -->
  <add key="TestFilePath" value="C:\root\files\resource\test"/>
  <add key="ImagePath" value="~/images/touryo/image.jpg"/>
  <!-- アプリケーションの使用するパラメータ - end -->
</appSettings>
