@using MVC_Sample.Models.ViewModels
@using MVC_Sample.Models.DataSets
@model CrudViweModel

@if (Model != null && Model.shippers != null)
{
                <table class="table">
                    <thead>
                        <tr>
                            <th>ShipperId</th>
                            <th>CompanyName</th>
                            <th>Phone</th>
                        </tr>
                    </thead>
                    <tbody>
                    @foreach (DsNorthwind.ShippersRow row in Model.shippers.Rows)
                    {
                        <tr>
                            <td>@row.ShipperID</td>
                            <td>@row.CompanyName</td>
                            <td>@row.Phone</td>
                        </tr>
                    }
                    </tbody>
                </table>
}
