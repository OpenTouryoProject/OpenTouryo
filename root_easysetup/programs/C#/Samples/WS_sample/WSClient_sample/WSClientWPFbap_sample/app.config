<?xml version="1.0"?>
<configuration>
  <appSettings>
    
    <!-- リッチクライアント（ClickOnce）用 -->
    <!-- フレームワークの使用するパラメータ - start -->
    <add key="FxMaxThreadCount" value="2"/>
    
    <!-- Webサービスの名前解決定義へのパス -->
    <!--リソース ファイルの場合-->
    <!--add key="FxXMLTMProtocolDefinition" value="TMProtocolDefinition.xml"/-->
    <!--埋め込まれたリソースの場合-->
    <add key="FxXMLTMProtocolDefinition" value="WSClientWPFbap_sample.TMProtocolDefinition.xml"/>
    
    <!-- メッセージ定義へのパス -->
    <!--リソース ファイルの場合-->
    <!--add key="FxXMLMSGDefinition" value="MSGDefinition.xml"/-->
    <!--add key="FxXMLSPDefinition" value="SPDefinition.xml"/-->
    <!--埋め込まれたリソースの場合-->
    <add key="FxXMLMSGDefinition" value="WSClientWPFbap_sample.MSGDefinition.xml"/>
    <add key="FxXMLSPDefinition" value="WSClientWPFbap_sample.SPDefinition.xml"/>

    <!-- フレームワークの使用するパラメータ - end -->

    <!-- 共通部品の使用するパラメータ - start -->

    <!-- Log4Netのコンフィグファイルへのパス -->
    <!-- リソース ファイルの場合 -->
    <!--add key="FxLog4NetConfFile" value="SampleLogConf2CS.xml" /-->
    <!-- 埋め込まれたリソースの場合 -->
    <add key="FxLog4NetConfFile" value="WSClientWPFbap_sample.SampleLogConf2CS.xml"/>

    <!-- 共通部品の使用するパラメータ - end -->
  </appSettings>
</configuration>
