<?xml version="1.0" encoding="UTF-8" ?>
<ROOT>
  UPDATE
    [Shippers]
  SET
    <DELCMA>
      <IF>[ShipperID] = @Set_ShipperID_forUPD,</IF>
      <IF>[CompanyName] = @Set_CompanyName_forUPD,</IF>
      <IF>[Phone] = @Set_Phone_forUPD,</IF>
    </DELCMA>
  WHERE
    [ShipperID] = @ShipperID
</ROOT>
